{
    "version": "0.2.6",
    "name": "self theme",
    "manifest_version":2, //这个是必须的，并且值为2
    "theme": {
      "images" : {
        "theme_frame" : "images/icon.png",
        "theme_frame_overlay" : "images/icon.png",
        "theme_toolbar" : "images/icon.png",
        "theme_ntp_background" : "images/icon.png",
        "theme_ntp_attribution" : "images/icon.png"
      },
      "colors" : {
        "frame" : [255, 0, 0],
        "toolbar" : [207, 221, 192],
        "ntp_text" : [20, 40, 0],
        "ntp_link" : [36, 70, 0],
        "ntp_section" : [0, 0, 0],
        "button_background" : [255, 0, 255]
      },
      "tints" : {
        "buttons" : [0, 0.1, 0]
      },
      "properties" : {
        "ntp_background_alignment" : "bottom"
      } 
    }
}